<section class="history hist-section">
    <h2 class="list-title" title="Click me!">{{ list.title }}</h2>
    <div class="collapse">    
        <p class="list-details">
            <em>{{ list.details }}</em>
        </p>
        <hr/>
        <ul class="list-bullets">
            {% for item in list.bullets %}
            <li>{{ item.description }}</li>
            {% endfor %}
        </ul>
    </div>
</section>